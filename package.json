{
    "name": "challenge-frontend",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "check-types": "turbo run check-types",
        "test": "turbo run test"
    },
    "devDependencies": {
        "prettier": "^3.7.4",
        "turbo": "^2.8.3",
        "typescript": "5.9.2"
    },
    "overrides": {
        "lodash": "4.17.23"
    },
    "engines": {
        "node": ">=22",
        "npm": ">=10"
    },
    "packageManager": "npm@10.9.2",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
